{"file":"CapabilityRadar.client-JavyGd5l.js","mappings":";;;;;;;;;;;;;;;AAgBA,QAAI,CAACA,YAAmB,kBAAkB,iBAAiB,cAAc,CAAC;AAoB1E,UAAM,QAAQ;AAKd,UAAM,uBAAuB;AAAA,MAC3B,EAAE,KAAK,oBAAoB,OAAO,OAAA;AAAA,MAClC,EAAE,KAAK,mBAAmB,OAAO,OAAA;AAAA,MACjC,EAAE,KAAK,kBAAkB,OAAO,OAAA;AAAA,MAChC,EAAE,KAAK,qBAAqB,OAAO,OAAA;AAAA,MACnC,EAAE,KAAK,sBAAsB,OAAO,OAAA;AAAA,IAAO;AAG7C,UAAM,SAAS,SAAS,MAAM;AAC5B,UAAI,CAAC,MAAM,QAAS,QAAO;AAC3B,YAAM,aAAa,qBAAqB,IAAI,CAAC,UAAU;AAAA,QACrD,MAAM,KAAK;AAAA,QACX,KAAK;AAAA,MAAA,EACL;AACF,YAAM,SAAS,qBAAqB,IAAI,CAAC,SAAS,MAAM,UAAU,KAAK,GAAG,KAAK,CAAC;AAChF,aAAO;AAAA,QACL,SAAS,CAAA;AAAA,QACT,OAAO;AAAA,UACL,WAAW;AAAA,UACX,WAAW,EAAE,WAAW,EAAE,OAAO,CAAC,uBAAuB,aAAa,IAAE;AAAA,UACxE,UAAU,EAAE,OAAO,UAAA;AAAA,QAAU;AAAA,QAE/B,QAAQ;AAAA,UACN;AAAA,YACE,MAAM;AAAA,YACN,QAAQ;AAAA,YACR,YAAY;AAAA,YACZ,WAAW,EAAE,OAAO,UAAA;AAAA,YACpB,WAAW,EAAE,OAAO,sBAAA;AAAA,YACpB,MAAM,CAAC,EAAE,OAAO,QAAQ;AAAA,UAAA;AAAA,QAC1B;AAAA,MACF;AAAA,IAEJ,CAAC;AAEc,aAAS,MAAM,MAAM,UAAU,OAAO;;;UA3EjC,OAAA,OAAM;;;AAGZ,cAAA,OAAAC,eAAAC,WAAA,EAAA,OAAM,2EAAuE,MAAA,CAAA,CAAA,iBAAA;AAAA;;;;;;;;;;","names":["EchartsRadarChart","_ssrRenderAttrs","_mergeProps"],"sources":["../../../../components/simulation/CapabilityRadar.client.vue"],"sourcesContent":["<template>\n  <ClientOnly v-if=\"option\">\n    <RadarChart :option=\"option\" autoresize class=\"w-full\" :style=\"{ height }\" />\n  </ClientOnly>\n  <div v-else class=\"flex h-full items-center justify-center text-xs text-muted-foreground\">\n    暂无能力画像\n  </div>\n</template>\n\n<script setup lang=\"ts\">\nimport { computed, defineAsyncComponent, h } from 'vue'\nimport { use } from 'echarts/core'\nimport { RadarChart as EchartsRadarChart } from 'echarts/charts'\nimport { TooltipComponent, LegendComponent } from 'echarts/components'\nimport { CanvasRenderer } from 'echarts/renderers'\n\nuse([EchartsRadarChart, TooltipComponent, LegendComponent, CanvasRenderer])\n\nconst RadarChart = defineAsyncComponent({\n  loader: () => (process.client ? import('vue-echarts') : Promise.resolve({ default: () => null })),\n  suspensible: false,\n  ssr: false,\n  loadingComponent: {\n    render() {\n      return h(\n        'div',\n        {\n          class:\n            'h-full w-full rounded-2xl border border-dashed border-border/60 bg-background flex items-center justify-center text-xs text-muted-foreground',\n        },\n        '雷达图加载中...'\n      )\n    },\n  },\n})\n\nconst props = defineProps<{\n  profile?: Record<string, number> | null\n  height?: string\n}>()\n\nconst capabilityDimensions = [\n  { key: 'risk_discernment', label: '风险识别' },\n  { key: 'info_protection', label: '信息保护' },\n  { key: 'response_speed', label: '响应速度' },\n  { key: 'emotional_control', label: '情绪稳定' },\n  { key: 'verification_skill', label: '核验能力' },\n]\n\nconst option = computed(() => {\n  if (!props.profile) return null\n  const indicators = capabilityDimensions.map((item) => ({\n    name: item.label,\n    max: 100,\n  }))\n  const values = capabilityDimensions.map((item) => props.profile?.[item.key] ?? 0)\n  return {\n    tooltip: {},\n    radar: {\n      indicator: indicators,\n      splitArea: { areaStyle: { color: ['rgba(15,15,15,0.04)', 'transparent'] } },\n      axisName: { color: '#6b7280' },\n    },\n    series: [\n      {\n        type: 'radar',\n        symbol: 'circle',\n        symbolSize: 4,\n        lineStyle: { color: '#0f0f0f' },\n        areaStyle: { color: 'rgba(15,15,15,0.15)' },\n        data: [{ value: values }],\n      },\n    ],\n  }\n})\n\nconst height = computed(() => props.height || '240px')\n</script>\n"],"version":3}